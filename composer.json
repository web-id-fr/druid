{
    "name": "webid/druid",
    "version": "0.1.0",
    "description": "Laravel Filament based CMS",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "web-id",
        "laravel",
        "cms",
        "content",
        "filament",
        "curator"
    ],
    "authors": [
        {
            "name": "Web ID team",
            "email": "team@web-id.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "larastan/larastan": "^2.8",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.20",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Webid\\Druid\\": "src/",
            "Webid\\Druid\\App\\": "src/app/",
            "Webid\\Druid\\Database\\Factories\\": "src/database/factories/"
        },
        "files": [
            "src/app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/app/",
            "Webid\\Druid\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webid\\Druid\\DruidServiceProvider"
            ]
        },
        "aliases": {
            "Druid": "Webid\\Druid\\Facades\\Druid"
        }
    },
    "scripts": {
        "pint": "pint",
        "phpstan": "phpstan --memory-limit=4G",
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}
