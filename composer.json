{
    "name": "webid/druid",
    "version": "0.1",
    "description": "Laravel Filament based CMS",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Webid\\Druid\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Web ID team",
            "email": "team@web-id.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.1",
        "filament/forms": "^3.1",
        "laravel/framework": "^10"
    },
    "scripts": {
        "phpcs": "phpcs",
        "pint": "pint",
        "phpstan": "phpstan --memory-limit=4G",
        "phpcbf": "phpcbf || true"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webid\\Druid\\DruidServiceProvider"
            ]
        }
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.8",
        "squizlabs/php_codesniffer": "^3.8",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.19"
    }
}
